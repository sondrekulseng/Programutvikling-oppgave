<AnchorPane stylesheets="@style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.oslomet.AdminController">
   <children>
      <MenuBar layoutY="1.0" prefHeight="25.0" prefWidth="1000.0">
         <menus>
            <Menu mnemonicParsing="false" text="File">
               <items>
                  <MenuItem mnemonicParsing="false" text="Close" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Edit">
               <items>
                  <MenuItem mnemonicParsing="false" text="Delete" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem mnemonicParsing="false" text="About" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <Text layoutX="427.0" layoutY="70.0" strokeType="OUTSIDE" strokeWidth="0.0" text=" Admin siden" textAlignment="CENTER">
         <font>
            <Font size="25.0" />
         </font>
      </Text>
      <TableView fx:id="tblKomponenter" editable="true" layoutY="166.0" prefHeight="243.0" prefWidth="1000.0">
         <columns>
            <TableColumn fx:id="navnCol" onEditCommit="#txtNavnEdited" prefWidth="124.00002801418304" text="Komponent">
               <cellValueFactory>
                  <PropertyValueFactory property="navn" />
               </cellValueFactory>
               <cellFactory>
                  <TextFieldTableCell fx:factory="forTableColumn" />
               </cellFactory>
            </TableColumn>
            <TableColumn fx:id="prisCol" onEditCommit="#doublePrisEdited" prefWidth="97.3333511352539" text="Pris">
               <cellValueFactory>
                  <PropertyValueFactory property="pris" />
               </cellValueFactory>
               <cellFactory>
                  <TextFieldTableCell fx:factory="forTableColumn" />
               </cellFactory>
            </TableColumn>
            <TableColumn fx:id="kategoriCol" prefWidth="127.33335876464844" text="Kategori">
               <cellValueFactory>
                  <PropertyValueFactory property="kategori" />
               </cellValueFactory>
            </TableColumn>
         </columns>
      </TableView>
      <Text layoutX="14.0" layoutY="110.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Filtrering">
         <font>
            <Font size="15.0" />
         </font>
      </Text>
      <Button layoutX="245.0" layoutY="442.0" mnemonicParsing="false" onAction="#btnNyKomponent" prefHeight="37.0" prefWidth="200.0" styleClass="btnConfirm" text="Legg til komponent">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button layoutX="560.0" layoutY="442.0" mnemonicParsing="false" onAction="#btnLoggUt" prefHeight="37.0" prefWidth="200.0" styleClass="btnCancel" text="Logg ut">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <ChoiceBox fx:id="velgKategori" layoutX="14.0" layoutY="127.0" onAction="#sorterTabell" prefWidth="150.0" value="Vis alle" />
      <Text layoutX="262.0" layoutY="109.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Sortering">
         <font>
            <Font size="15.0" />
         </font>
      </Text>
      <ChoiceBox fx:id="velgSortering" layoutX="262.0" layoutY="127.0" onAction="#sorterTabell" prefWidth="150.0" value="Alfabetisk" />
      <Button layoutX="524.0" layoutY="126.0" mnemonicParsing="false" onAction="#btnSlettRad" prefHeight="23.0" prefWidth="145.0" styleClass="btnConfirm" text="Slett valgt rad">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
